    <fields>
        <fullName>ManagerId</fullName>
        <trackFeedHistory>true</trackFeedHistory>
        <type>Hierarchy</type>
    </fields>
    <fields>
        <fullName>User_Id__c</fullName>
        <description>This is a user Id generated for every User</description>
        <displayFormat>UID-{0000}</displayFormat>
        <externalId>true</externalId>
        <label>User Id</label>
        <type>AutoNumber</type>
    </fields>
    <fields>
        <fullName>User_Profile__c</fullName>
        <externalId>false</externalId>
        <label>User Profile</label>
        <picklist>
            <picklistValues>
                <fullName>Researcher</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Data Steward</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Legal Reviewer</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>email_custom__c</fullName>
        <description>This is custom email field to avoid duplication of users</description>
        <externalId>false</externalId>
        <label>email_custom</label>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Email</type>
        <unique>true</unique>
    </fields>
    <listViews>
